<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>PaymentRequest</servlet-name>
    <servlet-class>com.legend.servlet.PaymentRequest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PaymentRequest</servlet-name>
    <url-pattern>/paymentrequest</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>PaymentResultResponse</servlet-name>
    <servlet-class>com.legend.servlet.PaymentResultResponse</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PaymentResultResponse</servlet-name>
    <url-pattern>/paymentresultresponse</url-pattern>
  </servlet-mapping> 
</web-app>